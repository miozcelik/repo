    <test name="Navigate to techtorialacademy.com">
        <parameter name="url" value="techtorialacademy"></parameter>
        <classes>
        <class name="selenium_class.TESTNGAnnotations.ParallelExecution2"></class>
        </classes>
    </test>
</suite>
